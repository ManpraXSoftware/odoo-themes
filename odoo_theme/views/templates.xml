<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="_assets_primary_variables" inherit_id="website._assets_primary_variables" priority="1">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/odoo_theme/static/src/scss/basic_theme.scss"/>
        </xpath>
    </template>
    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/odoo_theme/static/src/scss/basic_theme_backend.scss"/>
        </xpath>
    </template>
